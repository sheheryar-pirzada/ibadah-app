            "./assets/fonts/Amiri"
          ],
          "widgets": [
            {
              "id": "prayer_times",
              "displayName": "Prayer Times",
              "description": "Shows prayer times and countdown to next prayer",
              "supportedFamilies": [
                "systemSmall",
                "systemMedium",
                "systemLarge"
              ]
            },
            {
              "id": "allah_names",
              "displayName": "99 Names of Allah",
              "description": "Daily name from the 99 Names of Allah",
              "supportedFamilies": [
                "systemSmall",
                "systemMedium"
              ]
            },
            {
              "id": "dua_reflection",
              "displayName": "Reflection",
              "description": "So where are you going?",
              "supportedFamilies": [
                "systemMedium"
              ]
            },
            {
              "id": "dua_calm",
              "displayName": "Calm & Ease",
              "description": "In the remembrance of Allah do hearts find rest",
