{
  "expo": {
    "name": "Ibadah",
    "slug": "ibadah",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ibadah",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#000000",
    "ios": {
      "deploymentTarget": "16.2",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Needed to calculate Prayer times",
        "NSPhotoLibraryUsageDescription": "Used when you save or share content to your photo library or to apps like Instagram.",
        "UIBackgroundModes": [
          "audio"
        ],
        "LSApplicationQueriesSchemes": [
          "fbapi",
          "fb-messenger-api",
          "fbauth2",
          "fbshareextension",
          "instagram-stories",
          "twitter",
          "whatsapp"
        ]
      },
      "bundleIdentifier": "com.peersahab.ibadah",
      "appleTeamId": "9Y5Y87RDPF"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0F3D2C"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.peersahab.ibadah",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-fbsdk-next",
        {
          "appID": "3345730002248977",
          "displayName": "Ibadah App",
          "scheme": "fb3345730002248977",
          "advertiserIDCollectionEnabled": false,
          "autoLogAppEventsEnabled": false,
          "isAutoInitEnabled": false
        }
      ],
      [
        "react-native-share",
        {
          "ios": [
            "fb",
            "instagram",
            "twitter",
            "whatsapp"
          ],
          "android": [
            "com.facebook.katana",
            "com.instagram.android",
            "com.twitter.android",
            "com.whatsapp"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0F3D2C"
        }
      ],
      "expo-audio",
